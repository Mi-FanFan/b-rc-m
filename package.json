{
  "name": "b-rc-m",
  "version": "0.9.0",
  "description": "react components for budee front-end mobile project",
  "main": "./lib",
  "module": "./es",
  "scripts": {
    "start": "node scripts/start.js",
    "clean": "rimraf ./lib ./dist ./es",
    "dist": "gulp default",
    "prepare": "npm run build && npm run dist",
    "build": "npm run clean && npm run build:es && npm run build:cjs",
    "build:es": "cross-env BABEL_ENV=es babel ./components --out-dir ./es --copy-files",
    "build:cjs": "cross-env BABEL_ENV=cjs babel ./components --out-dir ./lib --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mi-FanFan/b-rc-m.git"
  },
  "homepage": "https://github.com/Mi-FanFan/b-rc-m",
